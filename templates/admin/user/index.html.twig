{% extends 'admin/adminbase.html.twig' %}
{% block  body %}
    <h1>{{ title }}</h1>
    <a href="{{ path('admin_user_create') }}">Регистрация пользователя</a><br>
    <h2>Список пользователей</h2>
    <ul>
        {% for user in users %}
        <li>{{ user.email }} | {{ user.roles|json_encode }}</li>
        {% endfor %}
    </ul>
{% endblock %}